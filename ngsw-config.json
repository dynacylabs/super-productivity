{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.json", "/*.css", "/*.js"]
      }
    },
    {
      "name": "fonts",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/fonts/**/*.woff2",
          "/assets/fonts/**/*.woff",
          "/assets/fonts/**/*.ttf",
          "/assets/fonts/**/*.css"
        ]
      }
    },
    {
      "name": "icons",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**/*.png",
          "/assets/icons/**/*.svg",
          "/assets/icons/**/*.xml",
          "/assets/icons/browserconfig.xml",
          "/assets/icons/safari-pinned-tab.svg",
          "/assets/icons/apple-touch-icon.png",
          "/assets/icons/favicon-16x16.png",
          "/assets/icons/favicon-32x32.png",
          "/assets/icons/favicon-192x192.png"
        ]
      }
    },
    {
      "name": "themes",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/themes/**"]
      }
    },
    {
      "name": "i18n",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/i18n/**/*.json"]
      }
    },
    {
      "name": "lazy-assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "!/assets/bundled-plugins/**",
          "!/assets/fonts/**",
          "!/assets/icons/**",
          "!/assets/themes/**",
          "!/assets/i18n/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    }
  ]
}
