{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.json", "/*.css", "/*.js"]
      }
    },
    {
      "name": "fonts",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/fonts/**/*.woff2",
          "/assets/fonts/**/*.woff",
          "/assets/fonts/**/*.ttf",
          "/assets/fonts/**/*.css"
        ]
      }
    },
    {
      "name": "icons",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**/*.png",
          "/assets/icons/**/*.svg",
          "/assets/icons/**/*.xml",
          "/assets/icons/**"
        ]
      }
    },
    {
      "name": "themes",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/themes/**"]
      }
    },
    {
      "name": "lazy-assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "!/assets/bundled-plugins/**",
          "!/assets/fonts/**",
          "!/assets/icons/**",
          "!/assets/themes/**"
        ]
      }
    }
  ]
}
