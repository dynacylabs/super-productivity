{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.json", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/fonts/**/*.woff2",
          "/assets/fonts/**/*.woff",
          "/assets/icons/**",
          "/assets/themes/**",
          "!/assets/bundled-plugins/**"
        ],
        "urls": []
      }
    },
    {
      "name": "lazy-assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "!/assets/bundled-plugins/**",
          "!/assets/fonts/**",
          "!/assets/icons/**",
          "!/assets/themes/**"
        ]
      }
    }
  ]
}
